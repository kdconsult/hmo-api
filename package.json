{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "dev": "vite",
    "start": "ng serve",
    "build": "vite build && vite build --ssr",
    "build:auth": "ng build auth && cp public/auth/index.html resources/views/auth.blade.php",
    "build:software": "ng build software && cp public/app/index.html resources/views/app.blade.php",
    "build-version": "ts-node -O '{\"module\":\"commonjs\"}' git.version.ts",
    "generate-software-types": "graphql-codegen --config frontend/projects/software/codegen.yaml",
    "watch-software-types": "graphql-codegen --config frontend/projects/software/codegen.yaml --watch",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.5",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.5",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "apollo-angular": "^4.2.1",
    "ng-recaptcha": "^11.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular/cli": "~15.1.6",
    "@angular/compiler-cli": "^15.1.0",
    "@angular/localize": "^15.1.5",
    "@angular/material-moment-adapter": "^15.2.0",
    "@auth0/angular-jwt": "^5.1.2",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^2.8.7",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@inertiajs/vue3": "^1.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~4.3.0",
    "@types/react": "^18.0.28",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/server-renderer": "^3.2.31",
    "autoprefixer": "^10.4.13",
    "chart.js": "^4.2.1",
    "crypto-js": "^4.1.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "laravel-echo": "^1.15.0",
    "laravel-vite-plugin": "^0.7.2",
    "moment": "^2.29.4",
    "ngx-json-viewer": "^3.2.1",
    "postcss": "^8.4.21",
    "pusher-js": "^8.0.1",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.9.4",
    "vite": "^4.0.0",
    "vue": "^3.2.41"
  }
}
